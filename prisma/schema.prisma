generator client {
  provider = "prisma-client-js"
  // output QOLMASIN!
}


datasource db {
  provider = "postgresql"
}

model Room {
  id             String    @id @default(cuid())
  name           String
  capacity       Int
  isActive       Boolean   @default(true)
  deletedAt      DateTime?

  createdAt      DateTime  @default(now())
  updatedAt      DateTime  @updatedAt

  @@index([deletedAt])
}